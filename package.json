{
  "name": "humanoid-ai-monorepo",
  "version": "1.0.0",
  "description": "Monorepo for humanoid AI project with frontend and backend",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run start",
    "dev:backend": "cd backend && uv run main.py",
    "build": "cd frontend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "echo \"No build step for backend\"",
    "start": "cd frontend && npm run serve",
    "install": "cd frontend && npm install",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && uv sync"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "uuid": "^13.0.0"
  }
}
